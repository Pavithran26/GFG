//Generate Binary Numbers
import java.util.*;
class Solution {
    public ArrayList<String> generateBinary(int n) {
        ArrayList<String> result = new ArrayList<>();
        Queue<String> queue = new LinkedList<>();
        queue.add("1");
        for (int i = 0; i < n; i++) {
            String curr = queue.poll();
            result.add(curr);
            queue.add(curr + "0");
            queue.add(curr + "1");
        }       
        return result;
    }
}
