//AND In Range
class Solution {
    public int andInRange(int l, int r) {
        int shift = 0;
        
        // Find the common prefix by right-shifting until l and r are equal
        while (l < r) {
            l >>= 1;
            r >>= 1;
            shift++;
        }
        
        // Left-shift back to get the result
        return l << shift;
    }
}